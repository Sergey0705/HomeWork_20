@model HomeWork_20.Models.UserLogin


@{
    ViewData["Title"] = "Вход";

}

<h2>Вход в приложение</h2>
<form method="post" asp-controller="Account" asp-action="Login"
      asp-route-returnUrl="@Model.ReturnUrl" asp-antiforgery="true">
    <div asp-validation-summary="ModelOnly"></div>
    <div>
        <label asp-for="LoginProp"></label><br />
        <input asp-for="LoginProp" />
        <span asp-validation-for="LoginProp"></span>
    </div>
    <div>
        <label asp-for="Password"></label><br />
        <input asp-for="Password" />
        <span asp-validation-for="Password"></span>
    </div>
    <div>
        <input class="btn btn-primary mx-1" type="submit" value="Войти" />
    </div>
</form>
